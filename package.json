{
  "name": "generator-jhipster-ai",
  "version": "0.0.1",
  "private": true,
  "description": "Description for Ai",
  "keywords": [
    "yeoman-generator",
    "jhipster-blueprint",
    "jhipster-8"
  ],
  "license": "UNLICENSED",
  "type": "module",
  "bin": {
    "jhipster-ai": "cli/cli.cjs"
  },
  "files": [
    "cli",
    "generators",
    "!**/__*",
    "!**/*.snap",
    "!**/*.spec.?(c|m)js"
  ],
  "scripts": {
    "ejslint": "ejslint generators/**/*.ejs",
    "lint": "eslint .",
    "lint-fix": "npm run ejslint && npm run lint -- --fix",
    "prepare": "husky",
    "prettier-check": "prettier --check \"{,**/}*.{md,json,yml,html,cjs,mjs,js,cts,mts,ts,tsx,css,scss,vue,java}\"",
    "prettier-format": "prettier --write \"{,**/}*.{md,json,yml,html,cjs,mjs,js,cts,mts,ts,tsx,css,scss,vue,java}\"",
    "pretest": "npm run prettier-check && npm run lint",
    "test": "vitest run",
    "update-snapshot": "vitest run --update",
    "vitest": "vitest"
  },
  "dependencies": {
    "generator-jhipster": "8.9.0"
  },
  "devDependencies": {
    "ejs-lint": "2.0.1",
    "eslint": "9.21.0",
    "eslint-config-prettier": "10.0.2",
    "eslint-plugin-prettier": "5.2.3",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "lint-staged": "15.4.3",
    "prettier": "3.5.3",
    "prettier-plugin-packagejson": "2.5.10",
    "vitest": "3.0.7",
    "yeoman-test": ">=8.2.0"
  },
  "engines": {
    "generator-jhipster": "8.9.0",
    "node": "^18.19.0 || >= 20.6.1"
  }
}
